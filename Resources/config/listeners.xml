<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
      xmlns:doctrine="http://symfony.com/schema/dic/doctrine">

    <parameters>
        <parameter key="egulias_quiz_events.class">Egulias\QuizBundle\EventListener\QuizListener</parameter>
    </parameters>

    <services>
        <service id="egulias.quiz.listener" class="%egulias_quiz_events.class%">
          <tag name="egulias.quiz.event_listener" event="egulias.quiz.pre_save_response"
            method="onPreSaveResponse" priority="2048" />
        </service>
    </services>

</container>
